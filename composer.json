{
  "name": "denniscuijpers/laravel-signed-token",
  "description": "Laravel Signed Token package",
  "homepage": "https://github.com/denniscuijpers/laravel-signed-token",
  "keywords": [
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Cuijpers",
      "email": "dennis@redacted.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "illuminate/support": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "DennisCuijpers\\SignedToken\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DennisCuijpers\\SignedToken\\SignedTokenServiceProvider"
      ],
      "aliases": {
        "Token": "DennisCuijpers\\SignedToken\\Facades\\Token"
      }
    }
  }
}
